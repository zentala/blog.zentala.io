{
  "name": "blog.zentala.io",
  "version": "1.0.0",
  "private": true,
  "contributors": [
    {
      "name": "Paweł Żentała",
      "email": "zentala@gmail.com",
      "url": "https://github.com/zentala"
    }
  ],
  "scripts": {
    "parcel:dev": " npx parcel src/index.html watch -p 2000 -d deploy/",
    "parcel:deploy:build": "npx parcel build src/index.html -d deploy/",
    "parcel:deploy:clean": "rm -rf deploy/",

    "deploy": "npm run deploy:clean && npm run build && npm run export && npm run deploy:setup && npm run deploy:push",
    "deploy:clean": "rm -rf out/",
    "deploy:setup": "npm run deploy:setup:cname && npm run deploy:setup:nojekyll",
    "deploy:setup:cname": "echo \"blog.zentala.io\" >> out/CNAME",
    "deploy:setup:nojekyll": "touch out/.nojekyll",
		"deploy:push": "npx gh-pages -d out -b deploy -t true",
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@fontsource/montserrat": "^4.3.0",
    "@fontsource/ubuntu": "^4.3.0",
    "font-awesome": "^4.7.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "engines": {
    "node": ">= 8.9.4",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "foreman": "^3.0.1",
    "gh-pages": "^3.1.0",
    "node-sass": "^6.0.0",
    "parcel-bundler": "^1.12.5",
    "postcss-modules": "^4.0.0",
    "scss": "^0.2.4",
    "@types/node": "17.0.22",
    "@types/react": "17.0.41",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "typescript": "4.6.2"
  }
}
